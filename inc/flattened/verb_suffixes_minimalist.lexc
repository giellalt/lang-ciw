!#####
!{VAI Conjunct suffixes
!#####

!Conjunct order
!See Valentine  2001:222, 236-237
!ibid 236-237 lacks nasal-final examples

!Insert schematic template here (also update style guide)

!stem-suffix interactions to be result of c1 in context:
!nc1si -> nzi 
!nc1[C] -> naC
!Vc1V -> VyV

!{Conjunct Order 
!{negative slot 
LEXICON VAI_CNJ_NEG 
+Neg:0        VAI_CNJ_NEG_SET ;
                VAI_CNJ_PERSNUM ;
@D.Passive@     VAI_CNJ_IRREG_NEG ; !this is a mite local for using a flag

LEXICON VAI_CNJ_IRREG_NEG
+Neg+3+Pl:%>si%>gwaa  # ; 

LEXICON VAI_CNJ_NEG_SET
@P.Negation.1@  VAI_CNJ_NEG_SPELLOUT ;

LEXICON VAI_CNJ_NEG_SPELLOUT
0:%>siiw3        VAI_CNJ_MODE_SET ;
0:%>siw3         VAI_CNJ_PERSNUM ;

LEXICON VAI_CNJ_MODE_SET 
@P.Mode.Yes@    VAI_CNJ_PERSNUM ; !This breaks from 0/1 for off/on
!} neg end

!{ persnum 

LEXICON VAI_CNJ_PERSNUM
@D.Passive@    VAI_CNJ_3 ; !no passive takes 'regular' 3
@R.Person@    VAI_PASSIVE_3 ; !passive 3 (no non-passive has a person flag)
@D.Person@    VAI_CNJ_DUB_EARLY_A ; !local passives are regular, non-passives pass

LEXICON VAI_CNJ_DUB_EARLY_A
0:w4    VAI_CNJ_DUB_SET_A ;
        VAI_CNJ_NOT_3 ;

LEXICON VAI_CNJ_DUB_SET_A
@P.Mode.Dub@    VAI_CNJ_NOT_3 ;

LEXICON VAI_CNJ_NOT_3
+1:%>aan       VAI_CNJ_PRT_1 ; !breaking morpheme over continuation lexica. have pret cnj "b" trigger assimilation, sometimes nasalize V
+2:%>an        VAI_CNJ_MODE_GET ; !ditto re assimilation: are there cases were nas-obs isn't modified to be homorganic?
+1+Pl:%>aang      VAI_CNJ_MODE_GET ;
+2+1+Pl:%>angw         VAI_CNJ_MODE_GET ;
+2+Pl:%>egw       VAI_CNJ_MODE_GET ;

LEXICON VAI_CNJ_3 
+3:%>d1               VAI_CNJ_NO_MODE_3PL_GATEWAY ; !d1 phon in root.lexc
+3+Pl:%>waad1       VAI_CNJ_3PL_NEG_GET ; !Irregularity when negated and this is last morpheme
+3+4:%>nid1           VAI_CNJ_NO_MODE_3OBV_GATEWAY ; !nas morph inid?
+X:%>nd1              VAI_CNJ_DUB_EARLY_B ; !nd1b -> ngib, nas morph ang?

LEXICON VAI_PASSIVE_3
+3:ind  VAI_PASSIVE_3PL ; !participles!

LEXICON VAI_PASSIVE_3PL
+Pl:waa VAI_CNJ_DUB_EARLY_B ;
        VAI_CNJ_MODE_GET ;

LEXICON VAI_CNJ_3PL_NEG_GET
@D.Negation@    VAI_CNJ_DUB_EARLY_B ;
@R.Negation@    VAI_CNJ_3PL_MODE_SET ;

LEXICON VAI_CNJ_3PL_MODE_SET
@P.Mode.Yes@ VAI_CNJ_DUB_EARLY_B ;

LEXICON VAI_CNJ_DUB_EARLY_B !letters or numbers for ordered? 
0:w4    VAI_CNJ_DUB_SET_B ;
        VAI_CNJ_MODE_GET ;

LEXICON VAI_CNJ_DUB_SET_B
@P.Mode.Dub@    VAI_CNJ_MODE_GET ;

!} person end

!{preterite slot 
LEXICON VAI_CNJ_PRT_1
+Prt:%>b1aan2   VAI_CNJ_DUB_GET_1 ; !rand doesn't show n as deletable, but Medler did
@D.Mode@    VAI_CNJ_DUB_GET_1 ; !Seeing siiw3 forces you to pick up mode suffixes
@R.Mode@    VAI_CNJ_DUB ;

LEXICON VAI_CNJ_DUB_GET_1
@D.Mode.Dub@    VAI_CNJ_1_END ;
@R.Mode.Dub@    VAI_CNJ_DUB ;

LEXICON VAI_CNJ_1_END
!         # ; !Medler didn't nasalize
0:h         # ; !left over morpheme chunk ... first person aan turns to aanh, pret turns to baanh

LEXICON VAI_CNJ_MODE_GET
            VAI_CNJ_PRT ;
@D.Mode@    #  ; !cannot pick up pcp if no mode (direct route to pcp already exists)

LEXICON VAI_CNJ_PRT 
+Prt:%>b1an2   VAI_CNJ_DUB_GET ;
@R.Mode.Dub@    VAI_CNJ_DUB ;
!@D.Mode@    VAI_CNJ_DUB_GET ;
!@R.Mode@    VAI_CNJ_DUB ; !otherwise, you can end, or pick up +Dub
!unclear why commented out section here behaved differently from  VAI_CNJ_PRT_1

LEXICON VAI_CNJ_DUB_GET
@D.Mode.Dub@    VAI_CNJ_PRT_I1  ; !check for participle
@R.Mode.Dub@    VAI_CNJ_DUB ;

LEXICON VAI_CNJ_PRT_I1
0:i1    VAI_CNJ_PCP_GET ; !pret participles pluralize with iig
                            !ideally i1 in b1an2i1 would delete before dub, but morpheme boundary triggers i1->ii, which is shortened to i
!} preterite end

!{dubitative slot 
LEXICON VAI_CNJ_DUB 
+Dub:%>en2 VAI_CNJ_PCP_GET ;

!} dub end
!{participles (redundant with nominal periph slot)
LEXICON VAI_CNJ_NO_MODE_3PL_GATEWAY
0:y1        VAI_CNJ_PCP_PL ;
@P.3Num.Pl@ VAI_CNJ_DUB_EARLY_B;

LEXICON VAI_CNJ_NO_MODE_3OBV_GATEWAY
0:y1        VAI_CNJ_PCP_OBV ;
@P.3Num.Obv@ VAI_CNJ_DUB_EARLY_B;

LEXICON VAI_CNJ_PCP_GET
@R.Ccnj@@R.3Num.Pl@     VAI_CNJ_PCP_PL ;
@R.Ccnj@@R.3Num.Obv@    VAI_CNJ_PCP_OBV ;
                # ;

LEXICON VAI_CNJ_PCP_PL
+Pcp+3+Pl:%>ag # ;

LEXICON VAI_CNJ_PCP_OBV
+Pcp+3+4:%>an # ;

!} participle end
!} cnj end 

