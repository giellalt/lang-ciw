!#####
!{ VAIO
!VAIO description in Valentine 2001 is incomplete (p 243) (no prt dub, no mode for neg, positive pcps but no conjunct)
!no passives
!recommended strategy for tag abstraction: open up object position for 3/0, fill subject position with remainder (don't fret about lack of theme sign)
LEXICON VAIO_ORDERS_GET
@D.Order.Cnj@@R.Person@ VAIO_INDEP_NEG ; 
@D.Person@    VAIO_NON_INDEP_GET ;

LEXICON VAIO_NON_INDEP_GET 
@D.Order.Cnj@   VAIO_IMPER_TAG ;
+Cnj:c1         VAI_CNJ_NEG ; !c1 handles stem-suffix specific adjustments
!vai cnj seems to be identical to VAIO cnj
!note: particples in VAIOs suggest /egw/, /angw/ for 2pl, 21pl (-og, -on periph suffs)
!this is already implemented from the looks of it :D
!might need to change it to w1 to get [o] in periphs

!{Imperative order
LEXICON VAIO_IMPER_TAG
+Imp:0 VAIO_IMPER_NEG ;

LEXICON VAIO_IMPER_NEG
            VAI_IMPER ;
+Neg:sii    VAIO_IMPER_NEG_DAA ; !only deviation from VAI_IMPER is (sii)daa[nig|nin]
            VAIO_IMPER_NEG_DAA ;

LEXICON VAIO_IMPER_NEG_DAA
+2+1+Pl:daany1 VAIO_IMPER_NEG_DAA_PERIPH ; 

LEXICON VAIO_IMPER_NEG_DAA_PERIPH
+3+Pl:%>ag # ; 
+%0+Pl:%>an # ; 
!}Imperative order end

!{Independent order
LEXICON VAIO_INDEP_NEG
+Neg:sii    VAIO_INDEP_PRENUM ; 
            VAIO_INDEP_PRENUM ; 

LEXICON VAIO_INDEP_PRENUM
0:n     VAIO_INDEP_SG_PREMODE ;
        VAIO_INDEP_PL_NUM_GET ;

LEXICON VAIO_INDEP_PL_NUM_GET
@D.Person.1@    VAIO_INDEP_PL_ILESS ;
@D.Person.3@    VAIO_INDEP_PL_IFULL ;

LEXICON VAIO_INDEP_PL_IFULL
+1+Pl:naan2y1   VAIO_INDEP_PERIPH_GET ;
+1+Pl:minaa   VAIO_INDEP_MODE ;

LEXICON VAIO_INDEP_PL_ILESS
+2+Pl:naawaa     VAIO_INDEP_PERIPH_GET ;

LEXICON VAIO_INDEP_SG_PREMODE
0:aa    VAIO_INDEP_MODE ;
        VAIO_INDEP_PERIPH_GET ;

LEXICON VAIO_INDEP_MODE
+Prt:ban2i1 VAIO_INDEP_PERIPH_GET ;
+Dub:dig VAIO_INDEP_DUB_PRE_PERIPH ;

LEXICON VAIO_INDEP_DUB_PRE_PERIPH
# ; !interp ...
0:en VAIO_INDEP_PERIPH_GET ; !interp ...

LEXICON VAIO_INDEP_PERIPH_GET
@D.Person.3@    VAIO_INDEP_PERIPH_3PL ;
@R.Person.3@    VAIO_INDEP_PERIPH_3OBV ;
                VAIO_INDEP_PERIPH ;

LEXICON VAIO_INDEP_PERIPH
+%0:0   # ;
+%0+Pl:%>an   # ;

LEXICON VAIO_INDEP_PERIPH_3OBV
+3+4:%>an # ;

LEXICON VAIO_INDEP_PERIPH_3PL
+3:%>an   # ;
+3+Pl:%>ag   # ;
!}Independent order end
!} VAIO end

